{
    "presets": [
        "react", [
            "env", {
                "modules": false
            }
        ]
    ],
    "plugins": [
        "transform-class-properties",
        "syntax-dynamic-import", 
        "transform-runtime",
        "transform-object-rest-spread",
        "babel-plugin-transform-decorators-legacy",
        ["import", 
            {
                "libraryName": "antd",
                "style": true
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}